{
  "name": "discord-gpt",
  "version": "1.0.0",
  "description": "A discord bot featuring ChatGPT and Dall-e APIs",
  "main": "src/app.ts",
  "type": "module",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "nodemon --watch './src/**' --ext 'ts' --exec 'npx tsc && node ./build/commands.js && node ./build/app.js'",
    "register": "npx tsc && node ./build/commands.js"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.0",
    "openai": "^3.3.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "nodemon": "^2.0.15",
    "typescript": "^5.1.6"
  }
}
